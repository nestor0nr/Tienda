@model Tienda.Data.Entities.State

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>Esta seguro de borrar?</h3>
<div>
    <partial name="_StateDetails" />

    <form asp-action="DeleteState">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-danger" />
        <a asp-action="Details" asp-route-id="@Model.country.Id" class="btn btn-success" />Regresar</a>
    </form>
</div>
